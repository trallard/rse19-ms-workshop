{
	"name": "bokehpreview",
	"displayName": "BokehPreview",
	"description": "Extension that allows preview of Bokeh served websites",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.38.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.bokehPreview"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.bokehPreview",
				"title": "Preview",
				"category": "Bokeh",
				"icon": {
					"light": "media/preview_light.svg",
					"dark": "media/preview_dark.svg"
				}
			}
		],
		"menus": {
			"editor/title": [
				{
				"when": "resourceFilename == main.py",
				"command": "extension.bokehPreview",
				"group": "navigation"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.38.0",
		"glob": "^7.1.4",
		"mocha": "^10.2.0",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.2.0"
	}
}
